[visual] # Visualizer filter

# Effects list (சரம்)
#effect-list=spectrum

# Video width (முழு எண்)
#effect-width=800

# Video height (முழு எண்)
#effect-height=500

# FFT window (சரம்)
#effect-fft-window=flat

# Kaiser window parameter (மிதவை)
#effect-kaiser-param=3.000000

# Show 80 bands instead of 20 (பூலியன்)
#visual-80-bands=1

# Draw peaks in the analyzer (பூலியன்)
#visual-peaks=1

# Enable original graphic spectrum (பூலியன்)
#spect-show-original=0

# Draw the base of the bands (பூலியன்)
#spect-show-base=1

# Base pixel radius (முழு எண்)
#spect-radius=42

# Spectral sections (முழு எண்)
#spect-sections=3

# V-plane color (முழு எண்)
#spect-color=80

# Draw bands in the spectrometer (பூலியன்)
#spect-show-bands=1

# Show 80 bands instead of 20 (பூலியன்)
#spect-80-bands=1

# Number of blank pixels between bands. (முழு எண்)
#spect-separ=1

# Amplification (முழு எண்)
#spect-amp=8

# Draw peaks in the analyzer (பூலியன்)
#spect-show-peaks=1

# Peak extra width (முழு எண்)
#spect-peak-width=61

# Peak height (முழு எண்)
#spect-peak-height=1

